{
  "name": "psychic-engine",
  "version": "1.0.0",
  "description": "Psychic Engine",
  "main": "app.ts",
  "type": "module",
  "scripts": {
    "build": "esbuild src/app.ts --bundle --platform=node --format=esm --outfile=dist/app.js --packages=external",
    "start": "tsx src/app.ts",
    "dev": "tsx watch src/app.ts",
    "start:prod": "node dist/app.js"
  },
  "dependencies": {
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "swagger-ui-express": "^5.0.1",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.5.2",
    "@types/swagger-ui-express": "^4.1.8",
    "esbuild": "^0.25.10",
    "tsx": "^4.20.6",
    "typescript": "^5.9.2"
  }
}
