{
  "name": "psychic-engine",
  "version": "1.0.0",
  "description": "Psychic Engine",
  "main": "app.ts",
  "type": "module",
  "scripts": {
    "build": "esbuild src/app.ts --bundle --platform=node --format=esm --outdir=dist --packages=external",
    "start": "node dist/app.js",
    "dev": "concurrently \"npm run build -- --watch\" \"nodemon dist/app.js --delay 500ms\""
  },
  "dependencies": {
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "tsc": "^2.0.4",
    "typescript": "^5.9.2",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.5.2",
    "concurrently": "^9.2.1",
    "esbuild": "^0.25.10",
    "nodemon": "^3.1.10"
  }
}
